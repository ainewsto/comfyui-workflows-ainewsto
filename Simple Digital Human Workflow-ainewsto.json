ilibili.com/1653942729\n我的工作流合集github地址：\n尽力用最简单的流程达到最想要的效果~~"],"color":"#432","bgcolor":"#653"}],"links":[[2,3,0,1,1,"SRT"],[3,1,0,4,0,"AUDIO"],[8,9,0,5,1,"SRT"],[9,10,0,11,0,"AUDIO"],[11,13,0,10,1,"SRT"],[12,14,0,10,2,"SRT"],[13,16,0,15,0,"AUDIO"],[14,15,0,17,0,"AUDIO"],[15,15,1,18,0,"AUDIO"],[58,8,0,5,0,"AUDIO"],[59,2,0,1,0,"AUDIO"],[60,12,0,10,0,"AUDIO"],[63,15,0,56,0,"AUDIO"],[64,56,0,58,0,"AUDIO"],[66,56,1,59,0,"AUDIO"],[67,60,0,61,0,"AUDIO"],[68,60,1,62,0,"AUDIO"],[69,56,0,60,0,"AUDIO"],[70,70,0,66,0,"MODEL"],[71,69,0,66,1,"CONDITIONING"],[72,69,1,66,2,"CONDITIONING"],[73,69,2,66,3,"LATENT"],[74,66,0,67,0,"LATENT"],[75,71,2,67,1,"VAE"],[77,71,1,69,0,"CLIP_VISION"],[78,72,0,69,1,"IMAGE"],[79,71,2,69,2,"VAE"],[80,71,0,70,0,"MODEL"],[94,87,0,83,0,"MODEL"],[95,86,0,83,1,"CONDITIONING"],[96,86,1,83,2,"CONDITIONING"],[97,86,2,83,3,"LATENT"],[98,83,0,84,0,"LATENT"],[99,88,2,84,1,"VAE"],[101,88,1,86,0,"CLIP_VISION"],[102,93,0,86,1,"IMAGE"],[103,88,2,86,2,"VAE"],[104,88,0,87,0,"MODEL"],[105,89,0,90,0,"MODEL"],[106,91,0,90,1,"CONDITIONING"],[107,92,0,90,2,"CONDITIONING"],[108,95,0,90,3,"LATENT"],[109,89,1,91,0,"CLIP"],[110,89,1,92,0,"CLIP"],[111,90,0,93,0,"LATENT"],[112,89,2,93,1,"VAE"],[113,93,0,94,0,"IMAGE"],[115,84,0,99,0,"IMAGE"],[152,137,0,133,0,"STRING"],[153,138,0,133,1,"STRING"],[155,133,0,136,0,"IMAGE"],[156,139,0,136,1,"VHS_AUDIO"],[157,135,0,137,0,"VHS_FILENAMES"],[158,138,0,139,0,"STRING"],[165,67,0,140,0,"IMAGE"],[167,143,0,142,0,"FACERESTORE_MODEL"],[168,133,0,142,1,"IMAGE"],[169,142,0,145,0,"IMAGE"],[170,139,0,145,1,"VHS_AUDIO"],[174,134,0,135,0,"IMAGE"]],"groups":[{"title":"UVR5 干声分离","bounding":[531,11,983,776],"color":"#3f789e","font_size":24,"locked":false},{"title":"声音推理","bounding":[1529,803,1347,634],"color":"#3f789e","font_size":24,"locked":false},{"title":"声音训练","bounding":[531,806,981,632],"color":"#3f789e","font_size":24,"locked":false},{"title":"TTS 推理","bounding":[2894,803,1312,635],"color":"#3f789e","font_size":24,"locked":false},{"title":"